{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "http://example.com/example.json",
	"type": "array",
	"id": "aspect",
	"idShort": "serialPartTypization",
	"version": "1.0.0",
	"semantic_id": "urn:bamm:io.catenax.serial_part_typization:1.1.0#SerialPartTypization",
	"title": "Serial Part Typizations",
	"description": "Serial Part Typizations",
	"items": {
		"type": "object",
		"required": [
			"part_instance_id",
			"manufacturing_date",
			"manufacturer_part_id",
			"name_at_manufacturer",
			"classification"
		],
		"dependentRequired": {
			"optional_identifier_key": [
				"optional_identifier_value"
			],
			"optional_identifier_value": [
				"optional_identifier_key"
			]
		},
		"properties": {
			"uuid": {
				"type": [
					"string",
					"null"
				],
				"pattern": "^urn:uuid:[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$",
				"title": "UUID URN",
				"examples": [
					"urn:uuid:8eea5f45-0823-48ce-a4fc-c3bf1cdfa4c2"
				]
			},
			"part_instance_id": {
				"type": ["string"],
				"title": "Part Instance ID",
				"minLength": 1,
				"examples": [
					"NO-159040131155901488695376"
				]
			},
			"manufacturing_date": {
				"type": ["string"],
				"format": "date-time",
				"title": "Manufacturing Date",
				"examples": [
					"2022-02-04T14:48:54"
				]
			},
			"manufacturing_country": {
				"type": [
					"string",
					"null"
				],
				"pattern": "[A-Z]{3}|null|^$",
				"title": "Manufacturing Country",
				"examples": [
					"DEU"
				]
			},
			"manufacturer_part_id": {
				"type": ["string"],
				"minLength": 1,
				"title": "Manufacturer Part ID",
				"examples": [
					"37754B7-76"
				]
			},
			"customer_part_id": {
				"type": [
					"string",
					"null"
				],
				"title": "Customer Part ID",
				"examples": [
					"37754B7-76"
				]
			},
			"classification": {
				"type": ["string"],
				"enum": [
					"product",
					"raw material",
					"software",
					"assembly",
					"tool",
					"component"
				],
				"title": "Classification",
				"examples": [
					"component"
				]
			},
			"name_at_manufacturer": {
				"type": ["string"],
				"minLength": 1,
				"title": "Name At Manufacturer",
				"examples": [
					"Sensor"
				]
			},
			"name_at_customer": {
				"type": [
					"string",
					"null"
				],
				"title": "Name At Customer",
				"examples": [
					"Sensor"
				]
			},
			"optional_identifier_key": {
				"type": [
					"string",
					"null"
				],
				"enum": [
					"VAN",
					"BatchID",
					""
				],
				"title": "Optional Identifier Key",
				"examples": [
					"VAN"
				]
			},
			"optional_identifier_value": {
				"type": [
					"string",
					"null"
				],
				"title": "Optional Identifier Value",
				"examples": [
					"Value"
				]
			}
		}
	},
	"examples": [
		{
			"uuid": "urn:uuid:8eea5f45-0823-48ce-a4fc-c3bf1cdfa4c2",
			"part_instance_id": "NO-159040131155901488695376",
			"manufacturing_date": "2022-02-04T14:48:54",
			"manufacturing_country": "DEU",
			"manufacturer_part_id": "37754B7-76",
			"customer_part_id": "37754B7-76",
			"classification": "component",
			"name_at_manufacturer": "Sensor",
			"name_at_customer": "Sensor",
			"optional_identifier_key": "VAN",
			"optional_identifier_value": "Value"
		}
	]
}